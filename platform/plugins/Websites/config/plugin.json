{
	"Q": {
		"pluginInfo": {
			"Websites": {
				"version": "0.8",
				"compatible": "0.8",
				"requires": {"Streams": "0.8"},
				"connections": ["Streams", "Websites"]
			}
		},
		"handlersBeforeEvent": {
			"Q/responseExtras": ["Websites/before/Q_responseExtras"],
			"Streams/Stream/save/Websites/bio": ["Websites/before/Streams_Stream_save_Websites_bio"]
		},
		"handlersAfterEvent": {
			"Streams/Stream/fetch/Websites/bio": ["Websites/after/Streams_Stream_fetch_Websites_bio"]
		}
	},
	"Streams": {
		"types": {
			"Websites/seo": {
				"create": true,
				"edit": true,
				"defaults": {
					"title": "Website SEO"
				}
			},
			"Websites/bio": {
				"create": true,
				"edit": true,
				"fields": ["userId", "bio"],
				"see": ["bio"],
				"defaults": {
					"title": "User Bio"
				}
			}
		}
	},
	"Websites": {
		"user": {
			"id": null
		}
	}
}
