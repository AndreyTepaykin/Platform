{
	"Q": {
		"routes": {
			"plugins/Places/:action": {"module": "Places"}
		},
		"pluginInfo": {
			"Places": {
				"version": "0.3",
				"compatible": "0.3",
				"connections": ["Places"],
				"requires": {"Streams": "0.8"}
			}
		}
	},
	"Streams": {
		"types": {
			"Places/location": {
				"create": true,
				"edit": true,
				"messages": {
					"Places/location/visit": { "post": true },
					"Places/location/stay": { "post": true }
				}
			},
			"Places/community": {
				"edit": true
			}
		}
	},
	"Places": {
		"nearbyZipcodes": {
			"limit": 100,
			"maxMiles": 100
		}
	}
}